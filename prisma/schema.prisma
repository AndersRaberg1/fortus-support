/// <reference types="@prisma/client" />

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model KbContent {
  id         Int      @id @default(autoincrement())
  title      String?
  category   String?
  content    String
  updated_at DateTime @updatedAt
}
